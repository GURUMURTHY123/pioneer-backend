#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/rlp/bin/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/rlp/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/node_modules:/home/workspace/nodejs/coding-assignments/assignment/node_modules:/home/workspace/nodejs/coding-assignments/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/rlp/bin/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/rlp/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules/@ethereumjs/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/@ethereumjs+rlp@4.0.1/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/pioneer-backend/node_modules:/home/workspace/nodejs/coding-assignments/assignment/backend-assignment/node_modules:/home/workspace/nodejs/coding-assignments/assignment/node_modules:/home/workspace/nodejs/coding-assignments/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/rlp" "$@"
else
  exec node  "$basedir/../../bin/rlp" "$@"
fi
